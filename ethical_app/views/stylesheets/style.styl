.navbar-label
  margin:0
  
.content-body
  margin-top:50px
 
h1 
  font-size: 4.0em
  text-align: center
  max-height: 100%
  left: 0
  right: 0
  font-family: "Courier New", sans-serif
  border-bottom-color: #ccc

h2
  margin-top: 1em
  font-size: 2.25em
  text-align: center
  //font-weight: bold
  padding-top: 1em
  padding-bottom: 1em
  font-family: "Courier New"
   
h3
  font-size: 1.75em
  font-family:'Muli',sans-serif
  font-weight:lighter
  text-align:center
  
p
  font-family: "Georgia"
  padding-bottom: 1.4em
  text-align: justify

@media screen and (max-width: 720px)
  h1
    font-size: 3.0em

  h2 
    font-size: 2.0em

  h3
    font-size: 1.5em

  p
    font-size: 1.2em

@media screen and (max-width: 640px)
  h1
    font-size: 2.5em

  h2
    font-size: 1.75em

  h3
    font-size: 1.25em

  p
    font-size: 1.0em


