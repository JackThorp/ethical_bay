border-radius(n)
  -webkit-border-radius: n
  -moz-border-radius: n
  -border-radius: n

.rounded-6
  border-radius(6px)

.rounded-12
  border-radius(12px)

.rounded-48
  border-radius(48px)

.rounded-20
  border-radius(20px)
  
.rounded-24
  border-radius(24px)

#nav-container
