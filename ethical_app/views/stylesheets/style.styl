*
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;

.link:hover
  background-color: #04945d

.navbar-default
  background-color: #fff
  background-image: none
  
.white_bg
  background-color:#fff

.lblue
  color:#0492b3

.lblue_bg
  background-color:#0492b3

.lgreen
  color:#85bd04

.red
  color:#eb2700

.red_bg
  background-color:#eb2700

.blue
  color:#465dc7

.blue_bg
  background-color:#465dc7

.green
  color:#04945d

.green_bg
  background-color:#04945d

.orange
  color:#ff8205

.orange_bg
  background-color:#ff8205

.purple
  color:#8c0b54

.purple_bg
  background-color:#8c0b54

border-radius(n)
  -webkit-border-radius: n
  -moz-border-radius: n
  -border-radius: n

.rounded-6
  border-radius(6px)

.rounded-12
  border-radius(12px)

.rounded-48
  border-radius(48px)

.rounded-20
  border-radius(20px)
  
.rounded-24
  border-radius(24px)

#nav-container
  padding-left:30px
  padding-right:30px
  
.navbar-label
  margin:0
  
.content-body
  margin-top:50px
 
h1 
  font-size: 4.0em
  text-align: center
  max-height: 100%
  left: 0
  right: 0
  font-family: "Courier New", sans-serif
  border-bottom-color: #ccc

h2
  font-size: 2.25em
  text-align: center
  padding-bottom:0.5em
  font-family: "Courier New"
   
h3
  font-size: 1.75em
  font-family:  'Muli',sans-serif 
  font-weight:lighter
  text-align:center
  
p
  font-family: "Helvetica Neue", Helvetica, Ariel, sans-serif
  font-size: 1.2em
  //text-align: justify
  
#wrapper
  max-width: 60em
  margin 0 auto

#list
  width 103.125%
  overflow hidden
  margin-left -1.562%
  margin-bottom -1.875em

.item 
  float: left
  width: 30.303%
  margin: 0 1.515% 1.875em

@media only screen and ( max-width: 40em ) /* 640px */
  .item
    width: 46.876% /* 305px */
    margin-bottom: 0.938em /* 15px */
      
@media only screen and ( max-width: 20em ) /* 640px */
  #list
    width: 100%
    margin-left: 0
  
  .item
    width: 100%
    margin-left: 0
    margin-right: 0

@media screen and (max-width: 720px)
  h1
    font-size: 3.0em

  h2 
    font-size: 2.0em

  h3
    font-size: 1.5em

  p
    font-size: 1.2em

@media screen and (max-width: 640px)
  h1
    font-size: 2.75em

  h2
    font-size: 1.8em

  h3
    font-size: 1.25em


